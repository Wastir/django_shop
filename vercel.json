{
    "builds": [
        {
            "src": "projekt/wsgi.py",
            "use": "@vercel/python",
        },
        { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" }}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "projekt/wsgi.py"
        }
    ]
}